{
  "name": "metastreams",
  "version": "0.1.2",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "description": "Readable and Writable Streams with buffering",
  "license": "MIT",
  "keywords": [
    "stream",
    "streams",
    "readable",
    "writable",
    "metarhia",
    "impress"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/metarhia/metastreams"
  },
  "main": "metastreams.js",
  "readmeFilename": "README.md",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && metatests ./test",
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \".*rc\" \"**/*.yml\""
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-metarhia": "^7.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.0",
    "metatests": "^0.7.1",
    "prettier": "^1.19.1"
  }
}
